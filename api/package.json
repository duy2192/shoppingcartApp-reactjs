  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "clean:win": "rmdir /s /q build && mkdir build",
    "build-babel": "babel ./src -d ./build/src build --source-maps inline --copy-files",
    "build": "npm run clean && npm run build-babel",
    "build:win": "npm run clean--window && npm run build-babel",
    "dev": "nodemon --config nodemon.json --exec babel-node src/index.js",
    "start": "Set NODE_ENV=production&&node build/src/index.js",
    "deploy": "export NODE_ENV=production&&pm2 delete build/src/index.js;pm2 start build/src/index.js",
    "test": "jest --watchAll"
  }
